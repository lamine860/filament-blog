{
    "name": "lamine860/filament-blog",
    "description": "Filament Blog Builder",
    "keywords": [
        "lamine860",
        "laravel",
        "filament-blog",
        "blog"
    ],
    "homepage": "https://github.com/lamine860/filament-blog",
    "license": "MIT",
    "authors": [
        {
            "name": "Lamine Ramata",
            "email": "mlamine860@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16.1",
        "filament/filament": "^3.0.49",
        "filament/forms": "^3.0.49",
        "filament/tables": "^3.0.49",
        "illuminate/contracts": "^10.0",
        "filament/spatie-laravel-tags-plugin": "^v3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13.1",
        "nunomaduro/collision": "^7.8.1",
        "nunomaduro/larastan": "^2.6.4",
        "orchestra/testbench": "^8.10.2",
        "pestphp/pest": "^2.18.2",
        "pestphp/pest-plugin-arch": "^2.3.3",
        "pestphp/pest-plugin-laravel": "^2.2",
        "pestphp/pest-plugin-livewire": "^2.1",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.14"
    },
    "autoload": {
        "psr-4": {
            "Stephenjude\\FilamentBlog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stephenjude\\FilamentBlog\\Tests\\": "tests",
            "Stephenjude\\FilamentBlog\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "lamine860\\FilamentBlog\\BlogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
